{
    "extends": "./tsconfig.app.json",
    "compilerOptions": {
        "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.lib.tsbuildinfo",
        "noEmit": false,
        "declaration": true,
        "outDir": "./dist",
        "rootDir": "./src/lib",
        "emitDeclarationOnly": true
    },
    "include": [
        "src/lib/**/*"
    ],
    "exclude": [
        "src/lib/**/*.test.*",
        "src/lib/**/*.spec.*"
    ]
}